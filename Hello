loadstring(game:HttpGet("https://raw.githubusercontent.com/Darkmoonxhubscript/DarkLibV2/refs/heads/main/Source.luau"))()

local Window = MakeWindow({Title = "[UPD] Blox fruits but very good"})

AddMinimizeButton({Icon = "10734897102"})

local Notify = NewNotify({
    Title = "Made By Sofietasa",
    Description = "Credits To DarkMoonHub For The Library",
    Time = 10
})

-- First Tab: Item Giver
local Tab1 = NewTab({Name = "Item Giver", Icon = "IconName"})

local Section1 = AddSection(Tab1, {Name = "Gives You The Tools"})

-- Dough Fruit Button
local Button1 = AddButton(Tab1, {
    Name = "Dough Fruit",
    Callback = function()
        local args = {
            [1] = "EMMFOSS__!ZCNSJNXCSDWQSANBX",
            [2] = "AddToolToBackpackKKK",
            [3] = {
                [1] = "Dough Fruit",
                [2] = game:GetService("Players").LocalPlayer.Backpack,
                [3] = true,
                [4] = true
            }
        }
        game:GetService("ReplicatedStorage").ALLREMBINDS.MainRemoteEvent:FireServer(unpack(args))
    end
})

-- Dragon East Fruit Button
local Button2 = AddButton(Tab1, {
    Name = "Dragon East Fruit",
    Callback = function()
        local args = {
